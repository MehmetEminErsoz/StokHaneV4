@model StokHaneV4.Models.TabHane

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="card shadow mb-4 ">
    <div class="card-header py-3"> <h6 class="m-0 font-weight-bold text-danger">Tavukhaneyi silmek istediğinize emin misiniz ?</h6></div>
    <div class="card-body">



        <div>
        
            <dl class="dl-horizontal">
                <dt>
                   Tavukhane Adı
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.TavukhaneAdi)
                </dd>

                <dt>
                   İşletme kod
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Tabisletme.isletmeKod)
                </dd>

            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Sil" class="btn btn-danger" /> 
                    <a href='@Url.Action("Index","TabHanes")' class="btn btn-primary">İptal</a>
                </div>
            }
        </div>

    </div>


</div>
